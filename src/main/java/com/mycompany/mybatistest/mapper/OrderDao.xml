<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.mycompany.mybatistest.dao.OrderDao">

	<select id="UserOrderListDao" resultType="com.mycompany.mybatistest.dto.UserOrderDto">
		SELECT * FROM usertbl u
		INNER JOIN ordertbl o
		ON u.userid = o.userid
		WHERE u.userid = #{userid }
	</select>
</mapper> 